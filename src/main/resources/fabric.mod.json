{
  "schemaVersion": 1,
  "id": "${id}",
  "name": "${name}",
  "description": "${description}",
  "contact": {
    "issues": "https://github.com/btwonion/SimpleAutoDrop/issues",
    "sources": "https://github.com/btwonion/SimpleAutoDrop"
  },
  "authors": [
    {
      "name": "btwonion"
    }
  ],
  "contributors": [],
  "license": ["GNU General Public License v3.0"],
  "version": "${version}",
  "environment": "client",
  "depends": {
    "fabric-language-kotlin": ">=1.8.3+kotlin.1.7.10",
    "fabric-api": ">=0.61.0+1.19.2",
    "silk-all": ">=1.9.2",
    "minecraft": ">=1.19.2"
  },
  "suggests": {
    "modmenu": "*"
  },
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "dev.nyon.simpleautodrop.MainKt::initClient"
      }
    ],
    "modmenu" : [
      "dev.nyon.simpleautodrop.config.ModMenuImpl"
    ]
  },
  "mixins": [
    "simpleautodrop.mixins.json"
  ],
  "custom": {
    "modmenu" : {
      "links": {
        "modmenu.discord": "https://discord.gg/pmHTtZnMd3"
      }
    }
  }
}